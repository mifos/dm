insert into loan_activity_details
select null id, a.created_by createdBy, a.account_id accountId, a.created_date createdDate,&apos;Loan Disbursal&apos; comments,
 la.loan_amount principal, ORIG_PRINCIPAL_CURRENCY_ID currency, 0 interest,
 ORIG_INTEREST_CURRENCY_ID curr, 0 penalty, ORIG_PENALTY_CURRENCY_ID curr, 0 fee, ORIG_FEES_CURRENCY_ID curr, 
la.loan_amount balancePrincipal, ORIG_PRINCIPAL_CURRENCY_ID curr, ls.orig_interest balanceInterest, ORIG_INTEREST_CURRENCY_ID curr, 
ls.orig_penalty balancePenalty, ORIG_PENALTY_CURRENCY_ID curr, ls.orig_fees balanceFee, ORIG_FEES_CURRENCY_ID curr
from loan_account la
inner join account a on la.account_id = a.account_id
inner join loan_summary ls on la.account_id = ls.account_id
where la.account_id &gt;${ket_maxAccountID}
and a.account_state_id in (5,6,9,7,8);

commit;

insert into loan_activity_details
select null id, a.created_by createdBy, a.account_id accountId, a.created_date createdDate,&apos;Payment rcvd.&apos; comments,
 0 principal, ORIG_PRINCIPAL_CURRENCY_ID currency, 0 interest,
 ORIG_INTEREST_CURRENCY_ID curr, 0 penalty, ORIG_PENALTY_CURRENCY_ID curr, ifnull(disb_fees.fees_disb,0) fee, ORIG_FEES_CURRENCY_ID curr, 
la.loan_amount balancePrincipal, ORIG_PRINCIPAL_CURRENCY_ID curr, ls.orig_interest balanceInterest, ORIG_INTEREST_CURRENCY_ID curr, 
ls.orig_penalty balancePenalty, ORIG_PENALTY_CURRENCY_ID curr, ls.orig_fees-ifnull(disb_fees.fees_disb,0) balanceFee, ORIG_FEES_CURRENCY_ID curr
from loan_account la
inner join account a on la.account_id = a.account_id
left join
(select sum(account_fees.account_fee_amnt) fees_disb,account_fees.account_id
from account_fees
inner join fee_frequency on account_fees.fee_id=fee_frequency.fee_id
where fee_frequency.frequency_payment_id=2
group by account_fees.account_id
) disb_fees
on disb_fees.account_id=a.account_id
inner join loan_summary ls on la.account_id = ls.account_id
where la.account_id &gt;${ket_maxAccountID}
and a.account_state_id in (5,6,9,7,8);

commit;


insert into loan_activity_details
select null id, t.personnel_id createdBy, t.account_id accountId, t.action_date createdDate,&apos;Payment rcvd.&apos; comments,
 lt.principal_amount principal, 
lt.PRINCIPAL_AMOUNT_CURRENCY_ID currency, 
lt.interest_amount interest,
 lt.PRINCIPAL_AMOUNT_CURRENCY_ID curr,
 0 penalty, 
 lt.PRINCIPAL_AMOUNT_CURRENCY_ID curr,
 0 fee, 
 lt.PRINCIPAL_AMOUNT_CURRENCY_ID curr, 
(select distinct(loan_summary.orig_principal) - sum(lt.principal_amount) 
from account_trxn innertbl 
inner join loan_summary on innertbl.account_id = loan_summary.account_id
inner join loan_trxn_detail lt on lt.account_trxn_id = innertbl.account_trxn_id
where innertbl.ACCOUNT_ID = t.ACCOUNT_ID and innertbl.account_trxn_id &lt;= t.account_trxn_id and innertbl.account_action_id = 1) prin_balance, 
lt.PRINCIPAL_AMOUNT_CURRENCY_ID curr, 
(select distinct(loan_summary.orig_interest) - sum(lt.interest_amount) 
from account_trxn innertbl 
inner join loan_summary on innertbl.account_id = loan_summary.account_id
inner join loan_trxn_detail lt on lt.account_trxn_id = innertbl.account_trxn_id
where innertbl.ACCOUNT_ID = t.ACCOUNT_ID and innertbl.account_trxn_id &lt;= t.account_trxn_id and innertbl.account_action_id = 1) int_balance, 
lt.PRINCIPAL_AMOUNT_CURRENCY_ID curr,
0 balancePenalty,  
lt.PRINCIPAL_AMOUNT_CURRENCY_ID curr, 
0 balanceFee, 
lt.PRINCIPAL_AMOUNT_CURRENCY_ID curr 
from account_trxn t
inner join loan_trxn_detail lt on lt.account_trxn_id = t.account_trxn_id
 where t.account_action_id = 1 and t.account_trxn_id &gt; ${ket_maxAccountTrxnID}
and t.installment_id&lt;&gt;1;


