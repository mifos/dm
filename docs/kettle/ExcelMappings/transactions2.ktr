    <sql>insert into account_payment
select null payment_id, account_id ACCOUNT_ID, 1 PAYMENT_TYPE_ID, principal_currency_id CURRENCY_ID, 
principal_paid+interest_paid+penalty_paid+misc_fees_paid+misc_penalty_paid+ifnull(total_fees.fee_amount,0) AMOUNT,&quot;&quot; RECEIPT_NUMBER,
null VOUCHER_NUMBER, null CHECK_NUMBER, payment_date PAYMENT_DATE, null RECEIPT_DATE, null BANK_NAME, null COMMENT
from loan_schedule
left outer join 
(select sum(loan_fee_schedule.amount) as fee_amount,id 
from
loan_fee_schedule
group by loan_fee_schedule.id) total_fees
on total_fees.id=loan_schedule.id
where loan_schedule.payment_date is not null and loan_schedule.account_id &gt; ${ket_maxAccountID};

commit;

insert into account_trxn
select null ACCOUNT_TRXN_ID, account_payment.account_id ACCOUNT_ID, payment_id PAYMENT_ID, 1 PERSONNEL_ID, 1 ACCOUNT_ACTION_ID, 
null CURRENCY_ID, account_payment.currency_id AMOUNT_CURRENCY_ID,amount AMOUNT, account_payment.payment_date DUE_DATE,&apos;-&apos; COMMENTS,
 account_payment.payment_date ACTION_DATE,
CURDATE() CREATED_DATE, account.customer_id CUSTOMER_ID, loan_schedule.installment_id INSTALLMENT_ID, null RELATED_TRXN_ID
from account_payment
inner join account on account.account_id = account_payment.account_id
inner join loan_schedule on loan_schedule.account_id = account_payment.account_id and loan_schedule.payment_date = account_payment.payment_date
where loan_schedule.payment_date is not null and account_payment.account_id &gt; ${ket_maxAccountID} and account_payment.payment_id &gt; ${ket_maxAccountPaymentID2};

commit;

insert into loan_trxn_detail
select account_trxn_id ACCOUNT_TRXN_ID, principal_paid PRINCIPAL_AMOUNT, PRINCIPAL_PAID_CURRENCY_ID,
interest_paid INTEREST_AMOUNT, INTEREST_PAID_CURRENCY_ID, penalty_paid PENALTY_AMOUNT, PENALTY_PAID_CURRENCY_ID,
misc_fees_paid MISC_FEE_AMOUNT, MISC_FEES_PAID_CURRENCY_ID, misc_penalty_paid MISC_PENALTY_AMOUNT, MISC_PENALTY_PAID_CURRENCY_ID
from account_trxn 
inner join loan_schedule on loan_schedule.account_id = account_trxn.account_id and 
loan_schedule.installment_id = account_trxn.installment_id
where account_action_id = 1 and payment_id &gt; ${ket_maxAccountPaymentID2};

commit;


insert into fee_trxn_detail
select null FEE_TRXN_DETAIL_ID,
account_trxn_id ACCOUNT_TRXN_ID,
loan_fee_schedule.account_fee_id,
loan_fee_schedule.AMOUNT_CURRENCY_ID FEE_AMOUNT_CURRENCY_ID,
loan_fee_schedule.amount as fee_amount
from account_trxn 
inner join loan_schedule on loan_schedule.account_id = account_trxn.account_id and loan_schedule.installment_id = account_trxn.installment_id
inner join loan_fee_schedule on loan_fee_schedule.id=loan_schedule.id
where account_action_id = 1 and account_trxn.payment_id &gt; ${ket_maxAccountPaymentID2}
and account_trxn.installment_id=1
and loan_schedule.payment_date is not null;

commit;

