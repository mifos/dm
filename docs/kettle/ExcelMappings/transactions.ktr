    <sql>insert into account_payment
select null payment_id, loan_account.account_id ACCOUNT_ID, 
1 PAYMENT_TYPE_ID, 
loan_amount_currency_id CURRENCY_ID, loan_amount-ifnull(disb_fees.fees_disb,0) AMOUNT,&quot;&quot; RECEIPT_NUMBER,
null VOUCHER_NUMBER, null CHECK_NUMBER, disbursement_date PAYMENT_DATE, null RECEIPT_DATE, null BANK_NAME, null COMMENT
from loan_account
inner join account on account.account_id=loan_account.account_id
left outer join 
(select sum(account_fees.account_fee_amnt) as fees_disb,account_fees.account_id
from account_fees 	
inner join fee_frequency on fee_frequency.fee_id=account_fees.fee_id
where fee_frequency.frequency_payment_id=2
group by account_fees.account_id
) as disb_fees
on disb_fees.account_id=account.account_id
where loan_account.account_id &gt; ${ket_maxAccountID}
and account.account_state_id in (5,9,6,7,8);

commit;

insert into account_trxn
select null ACCOUNT_TRXN_ID, account_payment.account_id ACCOUNT_ID, payment_id PAYMENT_ID, 1 PERSONNEL_ID, 10 ACCOUNT_ACTION_ID, 
null CURRENCY_ID, account_payment.currency_id AMOUNT_CURRENCY_ID,loan_account.loan_amount AMOUNT, payment_date DUE_DATE,&apos;-&apos; COMMENTS, payment_date ACTION_DATE,
CURDATE() CREATED_DATE, account.customer_id CUSTOMER_ID, 0 INSTALLMENT_ID, null RELATED_TRXN_ID
from account_payment
inner join account on account.account_id = account_payment.account_id
inner join loan_account on loan_account.account_id=account.account_id
where account_payment.account_id &gt; ${ket_maxAccountID} and account_payment.payment_id &gt; ${ket_maxAccountPaymentID};

commit;

insert into account_trxn
select null ACCOUNT_TRXN_ID, account_payment.account_id ACCOUNT_ID, payment_id PAYMENT_ID, 1 PERSONNEL_ID, 4 ACCOUNT_ACTION_ID, 
null CURRENCY_ID, currency_id AMOUNT_CURRENCY_ID,disb_fees.fees_disb AMOUNT, payment_date DUE_DATE,&apos;-&apos; COMMENTS, payment_date ACTION_DATE,
CURDATE() CREATED_DATE, account.customer_id CUSTOMER_ID, 0 INSTALLMENT_ID, null RELATED_TRXN_ID
from account_payment
inner join account on account.account_id = account_payment.account_id
inner join 
(select sum(account_fees.account_fee_amnt) as fees_disb,account_fees.account_id
from account_fees 
inner join fee_frequency on fee_frequency.fee_id=account_fees.fee_id
where fee_frequency.frequency_payment_id=2
group by account_fees.account_id
) as disb_fees
on disb_fees.account_id=account.account_id
where account_payment.account_id &gt; ${ket_maxAccountID} and account_payment.payment_id &gt; ${ket_maxAccountPaymentID};


commit;

insert into loan_trxn_detail
select account_trxn_id ACCOUNT_TRXN_ID, amount PRINCIPAL_AMOUNT, amount_currency_id PRINCIPAL_AMOUNT_CURRENCY_ID,
0.000 INTEREST_AMOUNT, amount_currency_id INTEREST_AMOUNT_CURRENCY_ID, 0.000 PENALTY_AMOUNT, amount_currency_id PENALTY_AMOUNT_CURRENCY_ID,
0.000 MISC_FEE_AMOUNT, amount_currency_id MISC_FEE_AMOUNT_CURRENCY_ID, 0.000 MISC_PENALTY_AMOUNT, amount_currency_id MISC_PENALTY_AMOUNT_CURRENCY_ID
from account_trxn 
where account_action_id = 10 and account_trxn.account_trxn_id &gt; ${ket_maxAccountTrxnID};

commit;

insert into loan_trxn_detail
select account_trxn_id ACCOUNT_TRXN_ID, 0.000 PRINCIPAL_AMOUNT, amount_currency_id PRINCIPAL_AMOUNT_CURRENCY_ID,
0.000 INTEREST_AMOUNT, amount_currency_id INTEREST_AMOUNT_CURRENCY_ID, 0.000 PENALTY_AMOUNT, amount_currency_id PENALTY_AMOUNT_CURRENCY_ID,
0.000 MISC_FEE_AMOUNT, amount_currency_id MISC_FEE_AMOUNT_CURRENCY_ID, 0.000 MISC_PENALTY_AMOUNT, amount_currency_id MISC_PENALTY_AMOUNT_CURRENCY_ID
from account_trxn 
where account_action_id = 4 and account_trxn.account_trxn_id &gt; ${ket_maxAccountTrxnID};

commit;

insert into fee_trxn_detail
select null FEE_TRXN_DETAIL_ID,
account_trxn_id ACCOUNT_TRXN_ID,
account_fees.account_fee_id ACCOUNT_FEE_ID,
account_trxn.amount_currency_id FEE_AMOUNT_CURRENCY_ID,
account_fees.account_fee_amnt FEE_AMOUNT
from account_trxn
inner join account_fees on account_trxn.account_id=account_fees.account_id
inner join fee_frequency on fee_frequency.fee_id=account_fees.fee_id 
where account_action_id = 4 and account_trxn.account_trxn_id &gt;${ket_maxAccountTrxnID}
and fee_frequency.frequency_payment_id=2;

commit;


insert into account_payment
(
ACCOUNT_ID,
PAYMENT_TYPE_ID,
CURRENCY_ID,
AMOUNT,
PAYMENT_DATE)
select customer_schedule.account_id,
1 payment_id,
MISC_FEES_CURRENCY_ID,
total_fees.amount,
customer_schedule.payment_date
from customer
inner join customer_schedule on customer.customer_id=customer_schedule.customer_id
inner join 
(select sum(customer_fee_schedule.amount) amount,customer_fee_schedule.id
from customer_fee_schedule
group by customer_fee_schedule.id
) total_fees
on total_fees.id=customer_schedule.id
where customer_schedule.payment_status=1
and customer_schedule.installment_id=1
and customer.customer_id &gt; ${ket_maxCustomerID};

commit;


insert into account_trxn
select null ACCOUNT_TRXN_ID, 
account_payment.account_id ACCOUNT_ID,
 payment_id PAYMENT_ID,
 1 PERSONNEL_ID,
 12 ACCOUNT_ACTION_ID, 
null CURRENCY_ID, 
account_payment.currency_id AMOUNT_CURRENCY_ID,
0 AMOUNT,
account_payment.payment_date DUE_DATE,
&apos;Payment rcvd.&apos; COMMENTS, 
account_payment.payment_date ACTION_DATE,
CURDATE() CREATED_DATE,
account.customer_id CUSTOMER_ID, 
1 INSTALLMENT_ID,
null RELATED_TRXN_ID
from account_payment
inner join account on account.account_id = account_payment.account_id
inner join customer_schedule on customer_schedule.account_id=account.account_id
inner join 
(select sum(customer_fee_schedule.amount) amount,customer_fee_schedule.id
from customer_fee_schedule
group by customer_fee_schedule.id
) total_fees
on total_fees.id=customer_schedule.id
where account_payment.payment_id &gt; ${ket_maxAccountPaymentID}
and customer_schedule.installment_id=1;

commit;

insert into customer_trxn_detail
(ACCOUNT_TRXN_ID,
TOTAL_AMOUNT,
TOTAL_AMOUNT_CURRENCY_ID,
MISC_FEE_AMOUNT,
MISC_FEE_AMOUNT_CURRENCY_ID,
MISC_PENALTY_AMOUNT,
MISC_PENALTY_AMOUNT_CURRENCY_ID
)
select account_trxn.account_trxn_id,
0,
AMOUNT_CURRENCY_ID,
0,
AMOUNT_CURRENCY_ID,
0,
AMOUNT_CURRENCY_ID
from account_trxn
where account_action_id=12
and account_trxn_id &gt; ${ket_maxAccountTrxnID};

commit;

insert into fee_trxn_detail
(ACCOUNT_TRXN_ID,
ACCOUNT_FEE_ID,
FEE_AMOUNT_CURRENCY_ID,
FEE_AMOUNT
)
select account_trxn.account_trxn_id,
customer_fee_schedule.account_fee_id,
account_trxn.AMOUNT_CURRENCY_ID,
customer_fee_schedule.amount
from account_trxn
inner join customer_schedule on account_trxn.account_id=customer_schedule.account_id
inner join customer_fee_schedule on customer_fee_schedule.id=customer_schedule.id
where account_trxn.account_trxn_id &gt; ${ket_maxAccountTrxnID};

commit;

insert into customer_account_activity
(ACCOUNT_ID,
DESCRIPTION,
AMOUNT,
FEE_AMOUNT_CURRENCY_ID,
CREATED_DATE,
CREATED_BY)
select account_trxn.account_id,
&apos;Payment rcvd.&apos;,
fee_cal.fee_amnt,
account_trxn.amount_currency_id,
date(curdate()),
1
from account_trxn
inner join 
(select sum(fee_trxn_detail.fee_amount) as fee_amnt,account_trxn_id
from fee_trxn_detail 
where account_trxn_id &gt; ${ket_maxAccountTrxnID}
group by account_trxn_id ) fee_cal
on account_trxn.account_trxn_id=fee_cal.account_trxn_id
where account_trxn.account_action_id=12
and account_trxn.account_trxn_id &gt; ${ket_maxAccountTrxnID};

commit;
</sql>
