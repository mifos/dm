    <sql>update loan_perf_history lph set NO_OF_PAYMENTS = (select count(*) from loan_schedule where payment_status = 1 
and account_id = lph.account_id) where lph.ACCOUNT_ID &gt; ${ket_maxAccountID};


update loan_perf_history lph set LOAN_MATURITY_DATE = (select max(action_date) from loan_schedule 
where account_id = lph.account_id) where lph.ACCOUNT_ID &gt; ${ket_maxAccountID};

update loan_perf_history lph set NO_OF_MISSED_PAYMENTS = (select count(*) from loan_schedule where payment_status = 0
and action_date &lt; CURDATE() and account_id = lph.account_id) where lph.ACCOUNT_ID &gt; ${ket_maxAccountID};

update client_perf_history cpf set total_active_loans = 
(select count(*) from account 
 where customer_id = cpf.customer_id and account_state_id in (5,9) 
and account.account_type_id = 1 and account.ACCOUNT_ID &gt; ${ket_maxAccountID})
where total_active_loans = 0;
</sql>
