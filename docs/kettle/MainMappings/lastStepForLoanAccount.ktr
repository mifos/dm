    <sql>insert into account_status_change_history 
select null,account_id,5,account_state_id,1,CURDATE() 
from account
where account_type_id  = 1 and account_id &gt; ${ket_maxAccountID};


insert into change_log
select null,
 1, 
&apos;mifos&apos;,
customer.customer_id,
12,
customer.customer_activation_date,
null
from customer
where customer_level_id=2;



insert into change_log
select null,
 1, 
&apos;mifos&apos;,
customer.customer_id,
1,
customer.customer_activation_date,
null
from customer
where customer_level_id=1;

insert into change_log_detail
select null,
change_log.change_log_id,
&apos;Status&apos;,
(case customer.status_id
when 9 then &apos;Active&apos;
when 12 then &apos;Closed&apos;
when 8 then &apos;Application Pending Approval&apos;
when 10 then &apos;On Hold&apos;
end) ,
(case customer.status_id
when 9 then &apos;Active&apos;
when 12 then &apos;Closed&apos;
when 8 then &apos;Application Pending Approval&apos;
when 10 then &apos;On Hold&apos;
end)
from change_log
inner join customer on customer.customer_id=change_log.entity_id
where change_log.entity_type=12;



insert into change_log_detail
select null,
change_log.change_log_id,
&apos;Status&apos;,
(case customer.status_id
when 3 then &apos;Active&apos;
when 6 then &apos;Closed&apos;
when 2 then &apos;Application Pending Approval&apos;
when 4 then &apos;On Hold&apos;
end) ,
(case customer.status_id
when 3 then &apos;Active&apos;
when 6 then &apos;Closed&apos;
when 2 then &apos;Application Pending Approval&apos;
when 4 then &apos;On Hold&apos;
end)
from change_log
inner join customer on customer.customer_id=change_log.entity_id
where change_log.entity_type=1;



</sql>
