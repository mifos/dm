    <sql>

drop table if exists CustomerClone;

CREATE TABLE CustomerClone LIKE Customer;

INSERT INTO CustomerClone SELECT * FROM Customer;


UPDATE Customer SET Customer.Search_Id = concat(&apos;1.&apos;, (select count(*) from CustomerClone where CustomerClone.customer_id &lt;= Customer.customer_id and CustomerClone.branch_id = Customer.branch_id
and ((CustomerClone.Customer_level_id = 3) OR (CustomerClone.Customer_level_id = 1 and CustomerClone.Group_Flag=0))))
	where Customer.CUSTOMER_LEVEL_ID = 3;


drop table if exists CustomerCenterClone;

CREATE TABLE CustomerCenterClone LIKE Customer;

INSERT INTO CustomerCenterClone SELECT * FROM Customer;


UPDATE Customer SET Customer.Search_Id = concat_ws(&apos;.&apos;,
	(SELECT Search_id from CustomerCenterClone where CustomerCenterClone.CUSTOMER_ID = Customer.PARENT_CUSTOMER_ID), 
	(select count(*) from CustomerCenterClone where CustomerCenterClone.customer_id &lt;= Customer.customer_id and 
	CustomerCenterClone.PARENT_CUSTOMER_ID = Customer.PARENT_CUSTOMER_ID and CustomerCenterClone.Customer_level_id = 2))
	where Customer.CUSTOMER_LEVEL_ID = 2 and customer.parent_customer_id is not null;


UPDATE Customer SET Customer.Search_Id = concat(&apos;1.&apos;, (select count(*) from CustomerClone where CustomerClone.customer_id &lt;= Customer.customer_id and CustomerClone.branch_id = Customer.branch_id
and CustomerClone.Customer_level_id = 2))
	where Customer.CUSTOMER_LEVEL_ID = 2 and customer.parent_customer_id is null;

drop table if exists CustomerGroupClone;

CREATE TABLE CustomerGroupClone LIKE Customer;

INSERT INTO CustomerGroupClone SELECT * FROM Customer;



UPDATE Customer SET Customer.Search_Id = concat_ws(&apos;.&apos;,
	(SELECT Search_id from CustomerGroupClone where CustomerGroupClone.CUSTOMER_ID = Customer.PARENT_CUSTOMER_ID), 
	(SELECT count(*) from CustomerGroupClone where CustomerGroupClone.customer_id &lt;= Customer.customer_id and 
	CustomerGroupClone.PARENT_CUSTOMER_ID = Customer.PARENT_CUSTOMER_ID and CustomerGroupClone.Customer_level_id = 1))
	where Customer.CUSTOMER_LEVEL_ID = 1 AND GROUP_FLAG=1;


drop table if exists CustomerGroupClone;

CREATE TABLE CustomerGroupClone LIKE Customer;

INSERT INTO CustomerGroupClone SELECT * FROM Customer;



UPDATE Customer SET Customer.Search_Id = concat(&apos;1.&apos;, (select count(*) from CustomerClone where CustomerClone.customer_id &lt;= Customer.customer_id and CustomerClone.branch_id = Customer.branch_id
and ((CustomerClone.Customer_level_id = 3) OR (CustomerClone.customer_level_id=2 and CustomerClone.parent_customer_id is null) OR (CustomerClone.Customer_level_id = 1 and CustomerClone.Group_Flag=0))))
	where Customer.CUSTOMER_LEVEL_ID = 1 AND GROUP_FLAG=0;






