<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="
http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>BootServlet</servlet-name>
		<servlet-class>org.mifos.dmt.Boot</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>MigrationHandler</servlet-name>
		<servlet-class>org.mifos.dmt.ui.MigrationHandler</servlet-class>
	<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>MigrationHandler</servlet-name>
		<url-pattern>/migrationHandle</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LogStream</servlet-name>
		<servlet-class>org.mifos.dmt.logging.LogStream</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogStream</servlet-name>
		<url-pattern>/logstream</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>WelcomeScreen</servlet-name>
		<servlet-class>org.mifos.dmt.ui.DispatcherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WelcomeScreen</servlet-name>
		<url-pattern>/mifosdmt</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CommonsFileUploadServlet</servlet-name>
		<servlet-class>org.mifos.dmt.ui.DMTExcelUpload</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CommonsFileUploadServlet</servlet-name>
		<url-pattern>/Commonsfileuploadservlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GenerateExcel</servlet-name>
		<servlet-class>org.mifos.dmt.ui.GenerateExcel</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GenerateExcel</servlet-name>
		<url-pattern>/generateExcel</url-pattern>
	</servlet-mapping>
</web-app>